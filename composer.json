{
  "name": "davidsierradz/php-soda",
  "description": "A PHP library for working with the Socrata API",
  "type": "library",
  "license": "MIT",
  "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/davidsierradz/PhpSoda"
      }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": "~5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "phpdocumentor/phpdocumentor": "2.*"
  },
  "autoload": {
    "psr-4": {
        "Davidsierradz\\Socrata\\": "src/"
    }
  },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      }
  },
  "authors": [
    {
      "name": "David Sierra DiazGranados",
      "homepage": "https://github.com/davidsierradz",
      "role": "Project Maintainer"
    }
  ],
  "support": {
    "issues": "https://github.com/davidsierradz/PhpSoda/issues",
    "irc": "irc://irc.freenode.net/socrata-soda",
    "wiki": "https://github.com/allejo/PhpSoda/wiki",
    "source": "https://github.com/davidsierradz/PhpSoda"
  }
}
